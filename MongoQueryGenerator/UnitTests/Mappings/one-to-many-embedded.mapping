__Solution__: "One to many embedded"
__Description__: ""
__Version__: "1.0"

########## ERModel ##########

Person {
	> personId: int
	name: string
}

Car {
	name: string
	year: int
}

Drives (Person, Car) {}

########## MongoDBSchema ##########

PersonDrivesCars < Person*, Car >
{
	_id: int < Person.personId >
	name: string < Person.name >
	cars_multivalued_ {
		name: string < Car.name >
		year: int < Car.year >
	}
}